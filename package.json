{
  "name": "create-tsnode-quickstart",
  "version": "1.0.0",
  "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
  "bin": "bin/cli.js",
  "main": "src/server.ts",
  "scripts": {
    "pm2:start": "pm2 start ecosystem.config.json",
    "build": "npx tsc",
    "start": "cross-env NODE_ENV=production node -r dotenv/config src/server.js",
    "server": "cross-env NODE_ENV=development nodemon -r dotenv/config --ignore \"src/**/*.test.ts\" src/server.ts",
    "migrate:make": "npx knex migrate:make create_skeleton_table --migrations-directory -x ts src/database/migrations",
    "migrate:latest": "npx knex migrate:latest --knexfile src/config/database.ts --env production",
    "migrate:rollback": "knex migrate:rollback --all --knexfile src/config/database.ts --env production",
    "migrate:up": "npx knex migrate:latest --knexfile src/config/database.ts --env development",
    "migrate:down": "knex migrate:rollback --all --knexfile src/config/database.ts --env development",
    "seed:make": "npx knex seed:make skeleton --knexfile -x ts src/config/database.ts",
    "seed:run": "npx knex seed:run --knexfile src/config/database.ts --env production",
    "seed": "npx knex seed:run --knexfile src/config/database.ts --env development",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "docker:prod": "cross-env NODE_ENV=production docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "cross-env NODE_ENV=development docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "cross-env NODE_ENV=development docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky"
  },
  "repository": "git@github.com:imVSaini/tsnode-quickstart.git",
  "author": "Vaibhav Saini",
  "license": "MIT",
  "keywords": [
    "node",
    "typescript",
    "express",
    "rest-api",
    "api-server",
    "ts-node",
    "backend",
    "knex",
    "mysql",
    "sqlite",
    "docker",
    "pm2",
    "eslint",
    "prettier",
    "jest",
    "supertest"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "5.0.1",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.13.0",
    "nodemailer": "^6.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "better-sqlite3": "^11.9.0",
    "concurrently": "^9.1.2",
    "coveralls": "^3.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
